
page{
	width: 100%;
	height: 100%;
	background-color: #f2f2f2;
}
.logo {
	height: 200rpx;
	width: 200rpx;
	margin: 200rpx auto 50rpx auto;
}
.text-area {
	display: flex;
	justify-content: center;
}
.title {
	font-size: 36rpx;
	color: #8f8f94;
}
.msg{
	height: 80rpx;
	display: flex;
	margin: 10rpx 10rpx;
	flex-direction: row;
	align-items: center;
	justify-content: flex-start;
	background-color: #FFFFFF;
	border-radius: 25rpx;
}
.msgicon {
	padding: 0 20rpx;
}
.msgtxt {
	font-size: 32rpx;
	color:#555555
}
.labpic{
}
.div1{
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
}
.appointmentlab{
	width: 48%;
	height: 300rpx;
	/* line-height: 350rpx; */
	background-color: #FFFFFF;
	margin: 10rpx 10rpx;
	text-align: center;
	border-radius: 5px;
	display: flex;
	justify-content: flex-start;
	flex-direction: column;
}
.appointmentlab image {
	width: 30%;
	height: 100rpx;
	margin-top: 40rpx;
}
.appointmentlab .textstyle{
	color:#767676 ;
	      /* 上下 左右 */
}

